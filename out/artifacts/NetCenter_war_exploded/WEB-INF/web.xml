<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>CustomerLogin</servlet-name>
        <servlet-class>com.netcenter.main.NetCenter.controller.LoginController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CustomerLogin</servlet-name>
        <url-pattern>/CustomerLogin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ImportarController</servlet-name>
        <servlet-class>com.netcenter.main.NetCenter.controller.ImportarController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ImportarController</servlet-name>
        <url-pattern>/Importar</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>

</web-app>